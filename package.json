{
    "name": "todo-example",
    "version": "0.0.0",
    "private": true,
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "@vitejs/plugin-vue": "^4.2.3",
        "@vitejs/plugin-vue-jsx": "^3.0.1",
        "concurrently": "^8.2.0",
        "dotenv": "^16.3.1",
        "esbuild": "^0.23.0",
        "eslint": "^8.53.0",
        "sass": "^1.63.6",
        "typescript": "5.2",
        "vite": "^4.4.3",
        "vite-node": "^2.0.4"
    },
    "scripts": {
        "frontend-dev": "concurrently --raw --kill-others \"vite\" \"tsc --noEmit --watch --incremental --preserveWatchOutput\"",
        "frontend-build": "vite build",
        "backend-dev": "node --inspect runner.mjs",
        "backend-build": "ucpem run build-backend",
        "build": "tsc --noEmit --incremental && yarn frontend-build && yarn backend-build",
        "serve": "vite preview",
        "lint": "yarn eslint ./",
        "get-ucpem": "curl -L https://github.com/bt7s7k7/UCPeM/releases/latest/download/ucpem.js -o node_modules/.bin/ucpem",
        "start": "cd build && yarn start"
    },
    "dependencies": {
        "@mdi/js": "^7.2.96",
        "@types/node": "^22.0.0",
        "vue": "^3.3.4",
        "vue-router": "^4.2.4"
    },
    "type": "module"
}
